{
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "script"
  },
  "env": {
    "node": true
  },
  "globals": {},
  "rules": {
    "strict": [
      2,
      "global"
    ],
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "curly": 2,
    "camelcase": 0,
    "eqeqeq": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "max-len": [
      2,
      {
        "code": 200,
        "ignoreComments": true
      }
    ],
    "new-cap": 1,
    "no-eq-null": 2
  }
}
